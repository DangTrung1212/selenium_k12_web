<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Regression Test" parallel="tests" thread-count="10">
    <test name="computer" parallel="tests" thread-count="10">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="test.order.TestOrderCheapComputer" />
        </classes>
    </test>
    <test name="Standard Computer" parallel="tests" thread-count="10">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="test.order.TestOrderStandardComputer" />
        </classes>
    </test>
</suite>